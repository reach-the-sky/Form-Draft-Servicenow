<?xml version="1.0" encoding="UTF-8"?><record_update table="sys_ui_page">
    <sys_ui_page action="INSERT_OR_UPDATE">
        <category>general</category>
        <client_script><![CDATA[function selectItem() {
	var selectedItem = document.getElementById('selectField').value;
	var callbackFunc = GlideDialogWindow.get().getPreference('onPromptComplete');

	var draftSelect = document.querySelector("#selectField");
	var offline = draftSelect.options[draftSelect.selectedIndex].getAttribute('offline');
	callbackFunc(selectedItem, offline == "true");
	GlideDialogWindow.get().destroy();
}

function cancelDialog() {
	GlideDialogWindow.get().destroy();
}]]></client_script>
        <description/>
        <direct>false</direct>
        <endpoint/>
        <html><![CDATA[<?xml version="1.0" encoding="utf-8" ?>
<j:jelly xmlns:j="jelly:core" xmlns:g="glide" xmlns:j2="null" xmlns:g2="glide.ui.template" xmlns:g3="null" xmlns:g4="null">

	<g:evaluate>
		var draftNames = RP.getWindowProperties().get('draftNames');
        if (draftNames == null || draftNames == "")
            draftNames = [];
        else
            draftNames = draftNames.split(',');
        var draftNamesOffline = RP.getWindowProperties().get('draftNamesOffline');
        if (draftNamesOffline == null || draftNamesOffline == "")
            draftNamesOffline = [];
        else
            draftNamesOffline = draftNamesOffline.split(',');
    </g:evaluate>

    <g:ui_table>
        <tr>
            <td style="padding:20px">
                <select id="selectField" size="16" name="selectField" style="width:1000px; padding:10px; background-color:#F5F5F5">
					<j2:forEach var="item" items="$[draftNames]">
						<g2:evaluate jelly="true">
							var draftName = jelly.item;
						</g2:evaluate>
						<option value="$[draftName]" offline="false" style="padding:5px; background-color:white">$[draftName]</option>
					</j2:forEach>
                    <j2:forEach var="item" items="$[draftNamesOffline]">
						<g2:evaluate jelly="true">
							var draftName = jelly.item;
						</g2:evaluate>
						<option value="$[draftName]" offline="true" style="padding:5px; background-color:white">$[draftName] - Offline]]>ðŸ“µ<![CDATA[</option>
					</j2:forEach>
                </select>
            </td>
        </tr>
        <tr>
           <td colspan="2" align="center" style="color:red;background-color:#dddddd;">          
               <span id='prompt_for_input_error'/>
           </td>
        </tr>
        <tr>
            <td colspan="2" align="right" style="padding:5px">
                <g:dialog_buttons_ok_cancel ok="selectItem();" />
            </td>
        </tr>
    </g:ui_table>
</j:jelly>
]]></html>
