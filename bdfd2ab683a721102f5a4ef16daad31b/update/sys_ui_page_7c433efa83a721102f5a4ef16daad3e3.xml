        <html><![CDATA[<?xml version="1.0" encoding="utf-8" ?>
<j:jelly xmlns:j="jelly:core" xmlns:g="glide" xmlns:j2="null" xmlns:g2="glide.ui.template" xmlns:g3="null" xmlns:g4="null">

	<g:evaluate>
        var items = RP.getWindowProperties().get('items').split(",");
    </g:evaluate>

    <g:ui_table>
		<tr>
            <td colspan="2" align="right">
                <img src="sn_flow_diagram_delete.png" alt="${gs.getMessage('Delete')}" onclick="deleteDraft()" width="20" height="20"/>
            </td>
        </tr>
        <tr>
            <td>
                <select id="selectField" size="6" name="selectField" style="width:300px;">
					<j2:forEach var="item" items="$[items]">
						<g2:evaluate jelly="true">
							var draftName = jelly.item;
						</g2:evaluate>
						<option value="$[draftName]">$[draftName]</option>
					</j2:forEach>
                </select>
            </td>
        </tr>
        <tr>
           <td colspan="2" align="center" style="color:red;background-color:#dddddd;">          
               <span id='prompt_for_input_error'/>
           </td>
        </tr>
        <tr>
            <td colspan="2" align="right">
                <g:dialog_buttons_ok_cancel ok="selectItem();" />
            </td>
        </tr>
    </g:ui_table>
</j:jelly>
]]></html>
